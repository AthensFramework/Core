{% extends "page/base.twig" %}

{% block head %}
    {# include none of the typical head #}
{% endblock head %}

{% block project_head %}
    {% for cssFile in projectCSS %}
        {# DOMPDF ignores baseHREF, so we have to insert it here #}
        <link rel="stylesheet" href="{{ baseHref }}/{{ cssFile }}">
    {% endfor %}
{% endblock project_head %}

{% block body %}
    {% for writable in writables  %}
        {{ writable|write|raw }}
    {% endfor %}
{% endblock body %}