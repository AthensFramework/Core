{% extends "page/mini-header.twig" %}

{% block head %}
    {{ parent() }}

    <link rel="stylesheet" href="vendor/athens/core/assets/css/multi-panel.css">
    <script type='text/javascript' src='vendor/athens/core/assets/js/multi-panel.js'></script>

{% endblock head %}

{% block content %}
    <div class="second-button" style="z-index:15" data-for-panel="2">
        <a href="#" class="cd-btn second-panel" data-for-panel="2">
            <button type="button" style="padding:0 5px">&#8657;&nbsp;<span class=message> </span>&nbsp;&#8657;</button>
        </a>
    </div>

    <div class="cd-panel from-right first-panel" data-for-panel="1" style="z-index:10">
        <div class="cd-panel-container first-panel" data-for-panel="1">
            <div class="cd-panel-content first-panel" data-for-panel="1">

                <div id="loadItHere"></div>
            </div> <!-- cd-panel-content -->
        </div> <!-- cd-panel-container -->
    </div> <!-- cd-panel -->


    <div class="cd-panel from-right second-panel" style="z-index:20" data-for-panel="2">
        <div class="cd-panel-container second-panel" data-for-panel="2">
            <div class="cd-panel-content second-panel" data-for-panel="2">
                <div id="loadItHere2"></div>
            </div> <!-- cd-panel-content -->
        </div> <!-- cd-panel-container -->
    </div> <!-- cd-panel -->

    {{ parent() }}
{% endblock content %}