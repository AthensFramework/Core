<div id={{ visibleFields|keys|merge(subForms|keys)|join|md5 }}
      class="subform {% if errors is not empty %}has-errors{% endif %}">

    {%  if errors is not empty %}
    <span class=form-errors><br>
        {% for error in errors %} {{ error }} {% endfor %}
    </span>
    {% endif %}

    <table>
        {% for fieldName, field in visibleFields %}
            {{ field|write|raw }}
        {% endfor %}
    </table>

</div>